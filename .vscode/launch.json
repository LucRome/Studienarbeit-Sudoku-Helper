            "module": "unittest",
            "cwd": "${workspaceFolder}/src/backend",
            "args": [
                // "sudoku.tests",
                // "validation.test_validation",
                // "algorithms.test_utils"
                "algorithms.test_completion"
            ],
            "justMyCode": true
        },
        {
            "name": "Python: Aktuelle Datei",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\src\\backend\\manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        },
        {
            "name": "Python: Frontend Tests",
            "type": "python",
            "request": "launch",
            "module": "unittest",
            "cwd": "${workspaceFolder}/src",
            
            "args": [
                // "frontend_tests.test_index",
                // "frontend_tests.test_verified",
                "frontend_tests.algorithm_pages.test_hidden_single",
            ],
            "justMyCode": true,
